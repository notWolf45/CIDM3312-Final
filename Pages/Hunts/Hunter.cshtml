 @page
 @model HunterModel
<h1>Lab 10</h1>
<hr />
<div class="row">
    <div class="col-6 border">
        <form method="post">
            <div class="form-group">
                <label asp-for="HunterName" class="control-label"></label>
                <input asp-for="HunterName" class="form-control" />
                <span asp-validation-for="HunterName" class="text-danger"></span>
            </div>
            <div class="form group">
                <input type="submit" value="Submit" class="btn btn-primary" />
            </div>
        </form>
    </div>
    <div class="col-6 border">
        <p>"Name @Model.FirstName @Model.LastName"</p>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}